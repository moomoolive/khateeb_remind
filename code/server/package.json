{
  "name": "backend",
  "version": "1.0.0",
  "description": "backend server for khateeb remind",
  "main": "Server.js",
  "private": true,
  "scripts": {
    "dev": "nodemon Server.js",
    "server": "node Server.js",
    "test:unit": "jest ./libraries/*",
    "test:int": "jest ./integration",
    "test": "jest jest ./libraries/requestValidation/",
    "test:debug": "jest sample.test.js",
    "docker:remove-old": "docker rm kr -f && docker image prune -f",
    "docker:create-test-container": "docker image build . -t khateeb-remind-test && docker create -p 80:80 --name kr khateeb-remind-test:latest",
    "docker:run-test-container": "docker container start kr -a",
    "docker:test": "npm run docker:remove-old && npm run docker:create-test-container && npm run docker:run-test-container",
    "docker:kill-test": "docker kill kr"
  },
  "author": "Mostafa Elbannan",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.13.0",
    "@aws-sdk/client-ses": "^3.13.0",
    "@aws-sdk/node-http-handler": "^3.12.0",
    "bcrypt": "^5.0.0",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "dayjs": "^1.10.3",
    "device-detector-js": "^2.2.9",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.9.2",
    "fast-deep-equal": "^3.1.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.1",
    "nanoid": "^3.1.20",
    "project-name-generator": "^2.1.9",
    "query-types": "^0.1.4",
    "web-push": "^3.4.4"
  },
  "devDependencies": {
    "jest": "^27.0.1",
    "nodemon": "^2.0.6",
    "supertest": "^6.1.3"
  }
}
